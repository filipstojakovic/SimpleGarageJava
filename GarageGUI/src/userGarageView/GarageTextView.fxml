      <Button layoutX="252.0" layoutY="470.0" mnemonicParsing="false" onAction="#removeCarButtonClicked" text="Remove Car" />
      <TextArea fx:id="platformTextArea" editable="false" layoutX="45.0" layoutY="39.0" prefHeight="410.0" prefWidth="343.0" promptText=" " style="-fx-text-alignment: center;">
         <font>
            <Font name="Bitstream Vera Sans Mono Bold" size="29.0" />
         </font>
      </TextArea>
      <TextField fx:id="textField" layoutX="416.0" layoutY="118.0" prefHeight="189.0" prefWidth="211.0" />
      <Button layoutX="466.0" layoutY="322.0" mnemonicParsing="false" onAction="#showTextFieldPlatform" text="show platform" />
